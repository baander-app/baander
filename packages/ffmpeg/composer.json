{
  "name": "baander/ffmpeg",
  "description": "ffmpeg library",
  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Baander\\Ffmpeg\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\FFMpegStreaming\\": "tests"
    }
  },
  "require": {
    "php": "^8.3 || ^8.4",
    "amphp/file": "^3.1",
    "php-ffmpeg/php-ffmpeg": "^1.3"
  }
}
