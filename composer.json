{
    "name": "martinjuul/baander",
    "type": "project",
    "description": "a media server project",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-redis": "*",
        "ext-zend-opcache": "*",
        "bacon/bacon-qr-code": "^3.0",
        "bepsvpt/blurhash": "^3.0",
        "beyondcode/laravel-server-timing": "^1.5",
        "dedoc/scramble": "^0.11.9",
        "ergebnis/clock": "^2.8",
        "genealabs/laravel-pivot-events": "^11.0",
        "guzzlehttp/guzzle": "^7.2",
        "hidehalo/nanoid-php": "^1.1",
        "intervention/image": "^3.5",
        "justinrainbow/json-schema": "^6.0",
        "laravel/fortify": "^1.23",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.25",
        "laravel/octane": "^2.4",
        "laravel/pail": "^1.1",
        "laravel/passport": "@dev",
        "laravel/pulse": "^1.0@beta",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-read-only": "^3.3",
        "league/mime-type-detection": "^1.15",
        "matomo/device-detector": "^6.3",
        "maxakawizard/json-collection-parser": "^1.10",
        "opcodesio/log-viewer": "^3.10",
        "php-ffmpeg/php-ffmpeg": "^1.2",
        "saloonphp/cache-plugin": "^3.0",
        "saloonphp/laravel-plugin": "^3.0",
        "saloonphp/saloon": "^3.0",
        "spatie/laravel-data": "^4.9",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-route-attributes": "^1.22",
        "spatie/laravel-sluggable": "^3.6",
        "spomky-labs/otphp": "^11.3",
        "stevebauman/unfinalize": "^2.1",
        "symfony/string": "^7.1",
        "thecodingmachine/safe": "^2.5",
        "tightenco/ziggy": "^2.2",
        "timokoerber/laravel-one-time-operations": "^1.4",
        "tpetry/laravel-postgresql-enhanced": "2.0.0",
        "web-auth/webauthn-lib": "^5.0",
        "xenos/musicbrainz": "@dev",
        "zend/php-reader": "@dev"
    },
    "require-dev": {
        "doctrine/dbal": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "inspector-apm/inspector-laravel": "^4.9",
        "laracraft-tech/laravel-schema-rules": "^1.4",
        "larastan/larastan": "^2.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "swoole/ide-helper": "~5.0.0",
        "xammie/mailbook": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Packages/Humanize/bytes.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse -c phpstan.neon"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./third_party/php-reader-1.8.1",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./third_party/music-brainz",
            "options": {
                "symlink": true
            }
        }
    ]
}
